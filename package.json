    "next": "14.1.3",
    "qs": "6.12.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-google-recaptcha": "3.1.0",
    "react-hook-form": "7.51.0",
    "react-icons": "5.0.1",
    "react-youtube": "10.1.0",
