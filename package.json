{
  "name": "looner-nextjs",
  "description": "Looner",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "dev:ssl": "next dev -H 0.0.0.0 --experimental-https",
    "build": "next build",
    "start": "next start",
    "gas": "next build && next start",
